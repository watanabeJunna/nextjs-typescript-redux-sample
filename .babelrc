{
    "presets": [
        "next/babel",
        "@zeit/next-typescript/babel"
    ],
    "plugins": [
        [
            "@babel/plugin-transform-typescript",
            {
                "allowNamespaces": true
            }
        ],
        [
            "styled-components",
            {
                "ssr": true,
                "preprocess": false
            }
        ]
    ]
}